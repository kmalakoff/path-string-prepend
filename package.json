{
  "name": "path-string-prepend",
  "version": "1.0.1",
  "description": "Prepends a path to a platform-specfic delimited path string and removes duplicate paths",
  "keywords": [
    "autonode",
    "prepend",
    "path"
  ],
  "homepage": "https://github.com/kmalakoff/path-string-prepend",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/path-string-prepend.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "main": "dist/cjs/index.cjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "docs": "tsds docs",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
